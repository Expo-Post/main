<b1>Error ID: <b><b1 id='error_id'><i>Processing...</i></b1></b></b1>
<script type="text/javascript">
    const id = document.getElementById('error_id');
    async function errorID() {
        try {
        var x = await fetch("https://api.expo-post.com/error", {
            "headers": {
                "Content-type":"application/json"
            },
            "method":"POST",
            "body": JSON.stringify({"code":"404", "text":`Error. CookiesJS: ${Cookies.get('error')}`})
        });
        var x = await x.json();
        var x = x.errorID;
        if(x == undefined) {id.innerText = "Something went wrong with the API, recieved undefined."} else {id.innerText = x;}
    } catch(err) {x.innerText = "API Offline. Error not recieved."}
    }
    errorID()
</script>
